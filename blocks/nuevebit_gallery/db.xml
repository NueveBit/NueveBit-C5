<?xml version="1.0"?>
<schema version="0.3">

	<table name="btNuevebitGallery">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="fsID" type="I"><unsigned /></field>		
		<field name="lazyLoad" type="L" ><unsigned /></field>		
		<field name="dataLayer" type="L" ><unsigned /></field>		
		<field name="playback" type="C" size="50"></field>	
		<field name="duration" type="I"><unsigned /></field>
		<field name="fadeDuration" type="I"><unsigned /></field>
	</table>

	<table name="btNuevebitGalleryImg">
		<field name="id" type="I">
			<key />
			<autoincrement />
			<unsigned />
		</field>
		<field name="bID" type="I"><unsigned /></field>
		<field name="fID" type="I"><unsigned /></field>		
		<field name="url" type="C" size="255"></field>
		<field name="title" type="C" size="255"></field>
		<field name="description" type="X"></field>
		<field name="duration" type="I"><unsigned /></field>
		<field name="fadeDuration" type="I"><unsigned /></field>
		<field name="groupSet" type="I"><unsigned /></field>
		<field name="position" type="I"><unsigned /></field>	
		<field name="imgHeight" type="I"><unsigned /></field>							
	</table>
	
</schema>
